<script type="text/javascript">
;(function($) {
    $(function() {
        var url=document.referrer;
        if ( url && ( url.search("http://")>-1 || url.search("https://")>-1 ) ) {
            var refurl =  url.match(/:\/\/(.[^/]+)/)[1];
            if(refurl.indexOf("baidu.com")>-1){
                $('#nobaidu_dlg').bPopup();
            }
        }
    });

})(jQuery);
</script>



